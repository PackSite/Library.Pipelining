{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "Enrich": [
      "WithMachineName",
      "WithEnvironmentUserName",
      "FromLogContext",
      "FromLogContext"
    ],
    "WriteTo": {
      "ConsoleSink": {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        }
      }
    }
  },
  "Pipelining": {
    "EnableProfiling": true,
    "ThrowOnReloadError": false,
    "Pipelines": {
      "demo-other": {
        "Enabled": true,
        "ContextType": "SampleApp.Pipelines.OtherDemoData.OtherDemoDataContext, SampleApp",
        "Description": "Other pipeline",
        "Lifetime": "Singleton",
        "Steps": [
          "SampleApp.Pipelines.OtherDemoData.OtherDemoDataStep1, SampleApp",
          "SampleApp.Pipelines.OtherDemoData.OtherDemoDataStep2, SampleApp"
        ]
      }
    }
  }
}
